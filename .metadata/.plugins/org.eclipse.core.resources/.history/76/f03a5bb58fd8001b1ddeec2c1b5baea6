package june0629;

class Parent{
	int x = 100;
	public void setvalue(int x) {
		this.x = x;
	}
	void method() {
		System.out.println("부모함수");
	}
}

class Child extends Parent() {
	int x = 200;
	Child(){x = 1000;}
	public 
}







