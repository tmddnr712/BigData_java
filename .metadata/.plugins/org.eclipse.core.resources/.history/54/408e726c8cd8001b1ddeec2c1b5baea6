package june0629;

class Calculation{
	int z;
	public void addition(int x, int y) {
		z = x+y;
		System.out.println("합계는: " + z);
	}
	public void subtraction(int x, int y) {
		z = x-y;
		System.out.println("두수의 차: " + z);
	}
}




