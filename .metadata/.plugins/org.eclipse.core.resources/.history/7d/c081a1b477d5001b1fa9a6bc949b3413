package june625;
// 메모리를 효율적으로 사용해야함
// 처리 속도를 고속
public class d_04_scope {
	int z = 100;
	static int t = 200;

	public static void main(String[] args) {
		int x = 100;
		// TODO Auto-generated method stub
		System.out.println(x);
		{
			int y = 100;
			System.out.println(y);
		}
		System.out.println(t);
	}
	d_04_scope d = new d_04_scope();
}
