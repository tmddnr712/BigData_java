package june625;

public class Student {
	String name;
	int kor;
	int mat;
	int eng;
	
	public void setStudentName(String name) {
		this.name = name;
	}
	public String getStudentName() {
		return name;
	}
	public Student(String name, int kor, int mat, int eng) {
		this.name = name;
		this.kor = kor;
		this.mat = mat;
		this.eng = eng;
	}
	
}
