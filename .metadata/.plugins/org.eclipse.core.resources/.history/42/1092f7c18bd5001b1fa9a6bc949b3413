package june625;

public class circle {
	public point po;
	public double radius;
	public point getPo() {
		return po;
	}
	public void setPo(point po) {
		this.po = po;
	}
	public double getRadius() {
		return radius;
	}
	public void setRadius(double radius) {
		this.radius = radius;
	}
	
	public circle() {}
	public Circle(point po, double radius) {
		this.po = po;
		this.radius = radius;
	}
	
	
	

}
