			System.out.println("mairadb 접속 성공!");
		}
		catch(Exception e) {e.printStackTrace();}
		PreparedStatement pstmt = null;
		ResultSet rs = null;
		try {
			String sql = "SELECT num, name, addr FROM member";
			pstmt = conn.prepareStatement(sql);
			
		}
	}

