package june625;

public class point {
	double x;
	double y;
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	
	public double getY() {
		return y;
	}
	public void setY(double y) {
		this.y = y;
	}
	
	public Point() {}
	public Point(double x, double y) {
		this.x = x;
		this.y = y;
	}
	public double distance() {
		return Math.sqrt(x*x + y*y);
	}

}
