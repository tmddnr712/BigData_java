package june625;

public class distance {
	int[] x1;
	int[] x2;
	int[] zero_point = {0, 0};
	
	public void setdistancex1(int[] x1) { // get/set 함수
		this.x1 = x1;
	}
	//get은 가져오고
	public int[] getdistancex1() {
		return x1;
	}
	
	public void setdistancex2(int[] x2) { // get/set 함수
		this.x2 = x2;
	}
	//get은 가져오고
	public int[] getdistancex2() {
		return x2;
	}
	
	public float zeroTodistance(int[] zero_point, int[] x1) {
		float result = (this.zero_point[0]-this.x1[0])^2 + (this.zero_point[1] - this.x1[1])^2;
		return result;
	}
}
