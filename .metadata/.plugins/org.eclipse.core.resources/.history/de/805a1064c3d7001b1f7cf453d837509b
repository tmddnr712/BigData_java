package june0628;
// 가위 바위 보 class
// 0   1  2
// comp, user, judge class 분할

class Comp {
	private int answer;
	public Comp(int answer) {
		this.answer = answer;
	}
	
	public int getgetComputer() {
		answer = ((int) (Math.random()*100)) %3;
		return answer;
	}

}

